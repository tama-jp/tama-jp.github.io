    <item>
      <title>Wails v2の初期設定</title>
      <link>http://localhost:62955/posts/wails-v2%E3%81%AE%E5%88%9D%E6%9C%9F%E8%A8%AD%E5%AE%9A/</link>
      <pubDate>Thu, 21 Jul 2022 22:43:00 +0900</pubDate>
